{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoicHJha2hhcmExIiwiYSI6ImNsbTYzMTBlNTBlNGczY2xneGt4aTVxbzcifQ.iJl8n48yA5RprniMngtCTg"
        }
      ]
    ],
    "name": "app",
    "slug": "app",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1253f9b1-a1df-488a-8e40-defce8633308"
      }
    },
    "android": {
      "package": "com.travelplanner.app",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.travelplanner.app"
    }
  }
}
